{
    "name": "mealdeal/email-verification",
    "description": "Email verification handler for MealDeal app",
    "require": {
        "php": ">=7.4",
        "kreait/firebase-php": "^6.0",
        "vlucas/phpdotenv": "^5.3",
        "phpmailer/phpmailer": "^6.10",
        "guzzlehttp/guzzle": "^7.0",
        "google/cloud-firestore": "^1.0",
        "google/cloud-storage": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MealDeal\\Auth\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    }
}
